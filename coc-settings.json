{
  "coc.preferences.formatOnSave": true,
  "list.previousKeymap": "<C-p>",
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    },
    "clangd": {
      "command": "clangd",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json"
      ],
      "filetypes": [
        "c",
        "cc",
        "cpp",
        "c++",
        "objc",
        "objcpp"
      ]
    },
    "lua": {
      "command": "lua-lsp",
      "filetypes": [
        "lua"
      ]
    }
  }
}
